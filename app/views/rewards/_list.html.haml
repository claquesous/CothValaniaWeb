%ol
  - points = {}
  - reward.character_rewards.includes(:member).unobtained.active.each do |cr|
    - points[cr] = cr.current_points
  - points = points.sort_by {|k,v| -v}
  - points.each do |member, points|
    %li
      = link_to member.member.name, member_path(member.member)
      = points.round(4)
